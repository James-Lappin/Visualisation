
@{
    ViewBag.Title = "Displaying The Location of Transactions";
    Layout = null;
}


<title>Simple Map</title>
<meta name="viewport" content="initial-scale=1.0">
<meta charset="utf-8">
<style>
    html, body {
        height: 100%;
        margin: 0;
        padding: 0;
    }

    #map {
        height: 100%;
    }
</style>

<!--Script references. -->
<!--The jQuery library is required and is referenced by default in _Layout.cshtml. -->
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<!--Reference the SignalR library. -->
<script src="~/Scripts/jquery.signalR-2.2.0.min.js"></script>

<!--Reference the autogenerated SignalR hub script. -->
<script src="~/signalr/hubs"></script>

@*<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js" type="text/javascript"></script>*@


@*<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD1w_OoKdB1llofVR93s5JSwGKLm4YFlHs"
        async defer></script>
    *@


<script src="https://maps.googleapis.com/maps/api/js?v=3.2&libraries=visualization&key=AIzaSyD1w_OoKdB1llofVR93s5JSwGKLm4YFlHs"></script>

<script type="text/javascript" src="~/Scripts/TypeScript/JudoMap.js"></script>
<script type="text/javascript" src="~/Scripts/TypeScript/MapHub.js"></script>

<div id="map"></div>


<script type="text/javascript">
    $(function initMap() {
        var map = new Mapping.JudoMap(document.getElementById('map'));
        var mapHub = new Mapping.MapHub(map);
        mapHub.start();
    });
</script>


